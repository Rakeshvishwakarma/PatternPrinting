// Pattern printing 
class Pattern{
    public static void main(String[] args){
        int i = 1;
        int n = 3;
        int count = 1;
        while (i <= n){
            int j = 1;
            
            while (j <= n){
                System.out.print(count);
                count += 1;
                j +=1;
                
            }
            System.out.println();
            i += 1;
        }
    }
}

// Output:- 123
//          456
//          789
