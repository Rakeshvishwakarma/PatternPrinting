class Pattern{
    public static void main(String[] args){
        int row = 1;
        int n = 5;
        while (row <= n){
            int column = 1;
            while (column <= row){
                System.out.print(row);
                column += 1;
            }
            System.out.println(" ");
            row += 1;
        }
    }
}


// Output:-  
//     1
//     22 
//     333 
//     4444 
//     55555
